{
  "name": "angular-mdc",
  "description": "Angular Material Design Components",
  "homepage": "https://github.com/trimox/angular-mdc-web",
  "bugs": "https://github.com/trimox/angular-mdc-web/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/trimox/angular-mdc-web.git"
  },
  "version": "0.6.2",
  "engines": {
    "node": ">= 5.4.1"
  },
  "scripts": {
    "postinstall": "ngc -p angular.tsconfig.json",
    "build:release": "gulp :mdc:build-release",
    "migrate-mdc-sass": "rimraf src/theme/material-components-web && node scripts/migrate-mdc-scss.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint 'src/**/*.ts'",
    "lint:css": "stylelint src/**/*.scss --config .stylelint-config.yaml",
    "fix:css": "stylelint --fix src/**/*.scss --config .stylelint-config.yaml",
    "test": "npm run test:unit",
    "test:unit": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "server": "webpack-dev-server --config config/webpack/webpack.dev.js",
    "start": "npm run server"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/platform-browser": "^5.0.2",
    "core-js": "^2.5.1",
    "material-components-web": "^0.26.0",
    "rxjs": "^5.5.2",
    "tsickle": "^0.24.x",
    "tslib": "^1.7.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^5.0.2",
    "@angular/flex-layout": "^2.0.0-beta.10",
    "@angular/http": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/platform-server": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@angular/upgrade": "^5.0.2",
    "@types/chalk": "^0.4.31",
    "@types/fs-extra": "^4.0.5",
    "@types/glob": "^5.0.33",
    "@types/gulp": "3.8.32",
    "@types/jasmine": "^2.8.2",
    "@types/minimist": "^1.2.0",
    "@types/node": "^7.0.21",
    "@types/run-sequence": "0.0.30",
    "angular2-template-loader": "^0.6.2",
    "chalk": "^1.1.3",
    "codecov": "^3.0.0",
    "codelyzer": "^4.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "focus-trap": "^2.3.1",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.0",
    "gulp-cli": "^1.4.0",
    "gulp-dom": "^0.9.17",
    "gulp-flatten": "^0.3.1",
    "gulp-highlight-files": "^0.0.5",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-transform": "^2.0.0",
    "gulp-util": "^3.0.8",
    "http-rewrite-middleware": "^0.1.6",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-webpack": "^2.0.6",
    "magic-string": "^0.22.4",
    "node-sass": "^4.7.2",
    "resolve-bin": "^0.4.0",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "run-sequence": "^1.2.2",
    "sass-loader": "^6.0.6",
    "scss-bundle": "^2.1.0",
    "sorcery": "^0.10.0",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.0.1",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.0.4",
    "tsconfig-paths": "^2.3.0",
    "tslint": "^5.8.0",
    "tsutils": "^2.6.0",
    "typescript": "~2.4.2",
    "uglify-js": "^2.8.14",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "config": {
    "validate-commit-msg": {
      "helpMessage": "%s\nNOTE: Please see angular's commit message guidelines (https://github.com/angular/angular.js/blob/master/CONTRIBUTING.md#commit) for information on how to format commit messages.\n\nAs an example, here is a valid commit message: 'docs(slider): Document slider public api'\n\nIf this commit is on a development / WIP branch, you can disable this by running `git commit --no-verify`.",
      "scope": {
        "allowed": [
          "button",
          "card",
          "checkbox",
          "core",
          "dialog",
          "drawer",
          "elevation",
          "fab",
          "form-field",
          "grid-list",
          "icon",
          "icon-toggle",
          "layout-grid",
          "linear-progress",
          "list",
          "material-icon",
          "menu",
          "radio",
          "ripple",
          "rtl",
          "select",
          "slider",
          "snackbar",
          "switch",
          "tabs",
          "textfield",
          "theme",
          "toolbar",
          "typography",
          "demo",
          "infrastructure",
          "package"
        ],
        "validate": true
      }
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "private": true
}
